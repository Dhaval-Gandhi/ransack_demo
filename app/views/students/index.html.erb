<div class="row">
  <h1>Student List</h1>
</div>
<div class="row">
  <%= search_form_for @q, remote: true do |f| %>
    <%= f.label :name_cont, 'Full Name' %>
    <%= f.search_field :name_cont %>
    <%= f.submit %>
  <% end %>
</div>
<div class="row" id="student_list">
  <%= render "student_list" %>
</div>
